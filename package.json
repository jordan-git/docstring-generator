{
    "name": "docstring-generator",
    "displayName": "Docstring Generator",
    "description": "Generate a template for your documentation comments by running a single command!",
    "version": "0.1.0",
    "publisher": "TOKEN GOES HERE WHEN REQUIRED",
    "license": "SEE LICENSE IN LICENSE.md",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "keywords": [
        "docstring",
        "generator",
        "comment",
        "document",
        "documentation"
    ],
    "activationEvents": [
        "onCommand:docstring-generator.generateDocstring"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "docstring-generator.generateDocstring",
                "title": "Generate Docstring"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.49.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^14.0.27",
        "eslint": "^7.9.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "dependencies": {
        "ts-dedent": "^1.1.1"
    },
    "bugs": {
        "url": "https://github.com/jordan-git/docstring-generator/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jordan-git/docstring-generator"
    }
}
